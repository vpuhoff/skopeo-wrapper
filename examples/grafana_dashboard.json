{
  "dashboard": {
    "id": null,
    "title": "Skopeo Wrapper Metrics",
    "tags": ["skopeo", "containers", "metrics"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Operations Total",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(skopeo_operations_total)",
            "legendFormat": "Total Operations"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": []
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Operations by Status",
